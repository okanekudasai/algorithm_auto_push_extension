// 문제 : odd-even-linked-list
// 결과 : Accepted / 속도: 45ms / 메모리 : 19.25MB
// 제출시각 : 24-01-12  11:58:43
class Solution:

    def oddEvenList(self, head: Optional[ListNode]) -> Optional[ListNode]:

        if head is None:

            return None



        odd = head

        even = head.next

        even_head = head.next



        while even and even.next:

            odd.next = odd.next.next

            even.next = even.next.next



            odd = odd.next

            even = even.next

        

        odd.next = even_head



        return head
